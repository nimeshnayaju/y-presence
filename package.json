{
  "version": "1.0.0",
  "private": true,
  "name": "y-presence",
  "description": "Add presence (live cursors/avatars) to your multiplayer application using yjs",
  "author": "@nimeshnayaju",
  "repository": {
    "type": "git",
    "url": "git@github.com:nimeshnayaju/y-presence.git"
  },
  "license": "MIT",
  "workspaces": [
    "examples/*",
    "packages/*"
  ],
  "keywords": [
    "yjs",
    "presence",
    "cursors",
    "avatars",
    "multiplayer"
  ],
  "scripts": {
    "start": "lerna run start --stream --parallel",
    "build:": "lerna run build --stream --parallel",
    "lint": "eslint src/ --ext .ts,.tsx",
    "clean": "rm -rf dist",
    "test": "lerna run jest",
    "test:ci": "lerna run jest:ci"
  },
  "devDependencies": {
    "@swc-node/jest": "^1.5.2",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "config": "^3.3.7",
    "esbuild": "^0.14.41",
    "eslint": "^8.16.0",
    "jest": "^28.1.0",
    "prettier": "^2.5.1",
    "tsconfig": "^7.0.0",
    "tslib": "^2.4.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.7.2"
  },
  "dependencies": {}
}
